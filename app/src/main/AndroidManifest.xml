<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.zkirtaem.licencjat" >

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.NFC" />

    <uses-feature
        android:name="android.hardware.nfc"
        android:required="true" />

    <application
        android:allowBackup="true"
        android:debuggable="true"
        android:icon="@mipmap/moja_ikonka"
        android:label="Obecności"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".addStudentActivity"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name=".addZajeciaActivity"
            android:label="@string/title_activity_add_zajecia"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.NoActionBar" >
        </activity>
        <activity android:name=".ZajeciaRozpoczeteActivity" >
        </activity>
        <activity
            android:name=".zajeciaRozpoczeteActivity"
            android:label="@string/title_activity_zajecia_rozpoczete"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.NoActionBar" >
        </activity>
        <activity
            android:name=".listObecnosciActivity"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name=".firstActivity"
            android:label="Obecności"
            android:icon="@mipmap/moja_ikonka"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.NoActionBar" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".settingsActivity"
            android:screenOrientation="portrait" >
        </activity>
        <activity android:name=".tabelaObecnosciActivity" >
        </activity>
    </application>

</manifest>
